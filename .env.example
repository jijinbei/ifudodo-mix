DISCORD_TOKEN=your_discord_bot_token_here
GUILD_ID=                              # Optional: guild ID for faster slash command sync

# Backend: "acestep" (ACE-Step 1.5) or "musicgen" (MusicGen, legacy)
GENERATOR_BACKEND=acestep

# MusicGen settings (only used when GENERATOR_BACKEND=musicgen)
MUSICGEN_MODEL=facebook/musicgen-small
DEVICE=cuda
DURATION=15
TOP_K=250
TEMPERATURE=1.0
CFG_COEF=3.0
REFERENCE_MELODY_PATH=assets/ifudodo_source.mp4

# ACE-Step settings (only used when GENERATOR_BACKEND=acestep)
ACESTEP_MODEL=acestep-v15-sft
ACESTEP_AUDIO_DURATION=180
ACESTEP_INFER_STEP=60

# Output
OUTPUT_FORMAT=wav
MAX_FILE_SIZE_MB=24
